{
  "name": "vibesheets",
  "version": "1.0.0",
  "description": "Production-ready timesheet tracking application",
  "main": "production-server.js",
  "scripts": {
    "start": "NODE_ENV=production node production-server.js",
    "dev": "NODE_ENV=development nodemon production-server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "build": "echo 'No build step required for static files'",
    "health": "curl -f http://localhost:3000/health || exit 1"
  },
  "keywords": [
    "timesheet",
    "tracking",
    "productivity",
    "work",
    "hours"
  ],
  "author": "VibeSheets Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "cors": "^2.8.5",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vibesheets/vibesheets.git"
  },
  "bugs": {
    "url": "https://github.com/vibesheets/vibesheets/issues"
  },
  "homepage": "https://vibesheets.com"
}